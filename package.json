{
  "name": "@idvidrine/create-express-app",
  "version": "1.0.6",
  "description": "Cloud native, production ready starter application for building RESTful APIs using Express and PostgreSQL",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "create-express-app": "bin/createApp.js"
  },
  "homepage": "https://github.com/ividrine/express-starter-app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ividrine/express-starter-app.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist/ && prisma generate && tsc && tsc-alias",
    "generate": "node --env-file=.env ./node_modules/prisma/build/index.js generate",
    "migrate-dev": "node --env-file=.env ./node_modules/prisma/build/index.js migrate dev",
    "lint": "eslint",
    "prod": "sh ./scripts/run-prod.sh",
    "dev": "sh ./scripts/run-dev.sh",
    "test": "sh ./scripts/run-tests.sh",
    "envup": "docker-compose up --build -d",
    "envdown": "docker-compose down",
    "prepare": "husky || true"
  },
  "keywords": [
    "node",
    "express",
    "postgresql",
    "prisma",
    "vitest",
    "docker"
  ],
  "author": "Isaac Vidrine",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/eslint-plugin-prettier": "^3.1.3",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/express-xss-sanitizer": "^2.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.6.2",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^7.2.0",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-logs": "^0.206.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@opentelemetry/winston-transport": "^0.17.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.2.0",
    "@prisma/instrumentation": "^7.2.0",
    "@scalar/express-api-reference": "^0.8.21",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "express-xss-sanitizer": "^2.0.0",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.5",
    "ora": "^8.2.0",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^9.0.1",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.17.0",
    "zod": "^4.0.14"
  }
}
